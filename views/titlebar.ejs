                </div>
                <div class="no-drag pointer navbar-btn" id="btn-styles"><i class="material-icons tiny">palette</i></div>
            </div>
            <div class="bottom-buttons">
                <div class="no-drag pointer navbar-btn" id="btn-config"><i class="fa fa-cog"></i></div>
            </div>
            <br/>
        </div>

        <ul class="filesNavbar" id="filesNavbarList">
